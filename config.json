{
  "name": "Mark Reference Objects for Retail",
  "type": "app",
  "categories": [
    "retail",
    "reference items"
  ],
  "description": "Prepare examples for products from catalog",
  "docker_image": "supervisely/base-py-sdk:6",
  "main_script": "src/main.py",
  "gui_template": "src/gui.html",
  "modal_template": "src/modal.html",
  "modal_template_state": {
    "catalogPath": "/retail/snacks_catalog.csv",
    "fieldName": "UPC",
    "columnName": "UPC CODE"
  },
  "task_location": "application_sessions",
  "isolate": true,
  "headless": true,
  "integrated_into": ["image_annotation_tool"],
  "icon": "https://img.icons8.com/color/96/000000/ingredients.png",
  "icon_background": "#FFFFFF",
  "context_menu": {
    "target": ["images_project"]
  }
}
